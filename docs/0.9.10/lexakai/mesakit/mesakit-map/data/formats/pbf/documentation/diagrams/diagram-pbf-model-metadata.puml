@startuml

    !include lexakai.theme
    
    title "PBF Metadata"
    
    Identifier <|-- PbfChangeSetIdentifier
    class PbfChangeSetIdentifier
    {
        --(constructors)--
        +PbfChangeSetIdentifier(long)
    }
    
    IntegerIdentifier <|-- PbfRevisionNumber
    class PbfRevisionNumber
    {
        --(constructors)--
        +PbfRevisionNumber(int)
    }
    
    IntegerIdentifier <|-- PbfUserIdentifier
    class PbfUserIdentifier
    {
        --(constructors)--
        +PbfUserIdentifier(int)
    }
    
    Name <|-- PbfUserName
    class PbfUserName
    {
        --(constructors)--
        +PbfUserName(String)
    }
    
@enduml

